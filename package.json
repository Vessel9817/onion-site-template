{
    "name": "website",
    "version": "0.0.0",
    "private": true,
    "license": "MIT",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "npx ts-node utils/build.ts",
        "start:webserver": "npx ts-node utils/webserver.ts",
        "broker": "docker compose up -d kafka",
        "producer": "npx ts-node \"./utils/kafka/producer.ts\"",
        "consumer": "npx ts-node \"./utils/kafka/consumer.ts\"",
        "create_topic": "docker exec -i broker bash /opt/kafka/bin/kafka-topics.sh --create --bootstrap-server localhost:9092 --topic",
        "nginx:dev": "docker compose --profile development up -d",
        "nginx:prod": "docker compose --profile production up -d",
        "onionscan": ".\\utils\\onionscan",
        "prettier": "npx prettier --write **/*.{js,jsx,ts,tsx,json,css,scss,html}",
        "eslint": "npx eslint . -c \"./config/eslint/eslint.config.cjs\"",
        "eslint:fix": "npm run eslint --fix",
        "tsc": "npx tsc --noEmit"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Vessel9817/website.git"
    },
    "dependencies": {
        "@confluentinc/kafka-javascript": "^1.2.0"
